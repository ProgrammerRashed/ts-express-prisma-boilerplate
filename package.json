{
  "name": "prisma-curd-server",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "prod": "NODE_ENV=production node ./dist/server.js",
    "build": "tsc",
    "start": "node dist/server.js",
    "format": "prettier --write .",
    "studio": "npx prisma studio",
    "postinstall": "prisma generate",
    "swagger": "ts-node src/swagger.ts"
  },
  "_moduleAliases": {
    "@utils": "src/utils",
    "@helpers": "src/helpers",
    "@middleware": "src/middlewares",
    "@config": "src/config"
  },
  "author": "Rashed",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "prisma": "^6.9.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.3",
    "swagger-autogen": "^2.23.7",
    "winston": "^3.17.0",
    "zod": "^3.25.53"
  }
}
